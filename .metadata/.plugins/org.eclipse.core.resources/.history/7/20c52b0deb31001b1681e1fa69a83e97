package com.livelightlabs.livelightgames.ligamarket.converter;

import org.springframework.core.convert.converter.Converter;

import com.livelightlabs.livelightgames.ligamarket.entity.team.Team;

public class TeamConverter implements Converter<com.livelightlabs.livelightgames.ligamarket.model.team.Team, Team> {

	@Override
	public Team convert(com.livelightlabs.livelightgames.ligamarket.model.team.Team source) {
		Team team = new Team(source.getAbbreviation(), source.getBadge(), source.getBanner(), country, source.getFacebook(), source.getFanart1(), source.getFanart2(),
				source.getFanart3(), source.getFanart4(), source.getInstagram(), source.getJersey(), source.getLogo(), source.getManager(), source.getName(), source.getSport(),
				source.getStadium(), source.getStadiumCapacity(), source.getStadiumImage(), source.getStadiumLocation(), source.getTwitter(), source.getWebsite(), source.getYear(), source.getYoutube())
		return null;
	}
}